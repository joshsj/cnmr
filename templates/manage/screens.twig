{% extends "manage/main.twig" %} {% block content %}
<h1>Table: Screen</h1>
{% for screen in screens %}
<ul>
  <li>
    <a href="/manage/screens/{{ screen.cinema_id }}/{{ screen.id }}"
      >Screen {{[screen.no, screen.area, screen.city]|join(", ") }}</a
    >
  </li>
</ul>
{% endfor %} {% endblock %}
